{
  "resources": {
    "head": [
      {
        "type": "script",
        "src": "./app.js"
      },
      {
        "type": "style",
        "src": "./app.css"
      }
    ]
  },
  "preview": {
    "handlers": [
      {
        "options": ["*"],
        "execute": "INSTALL_SCOPE.setOptions(INSTALL_OPTIONS)"
      }
    ]
  },
  "options": {
    "properties": {
      "element": {
        "title": "Location",
        "description": "Where would you like the push button to appear?",
        "order": 1,
        "type": "object",
        "format": "element",
        "default": {
          "selector": "body",
          "method": "append"
        }
      }
    }
  }
}
